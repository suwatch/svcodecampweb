    <script type="text/javascript" language="javascript">
        $(document).ready(function () {

            // Sponsors accordion
            var sponsorAccordionList = $('.sponsorsListWrap dl > dd').hide(),
                sponsorAccordionTitle = $('.sponsorsListWrap dl > dt');

            $('.sponsorsListWrap dl > dt > a').click(function () {
                var linkTarget = $(this),
                  targetTitle = linkTarget.parent(),
                  targetList = linkTarget.parent().next();

                // Check if it's expanded
                if (!targetList.hasClass('active')) {
                    // Close previous expanded panel
                    sponsorAccordionList.removeClass('active').slideUp();
                    sponsorAccordionTitle.removeClass('active');
                    // Expanded current panel
                    targetTitle.addClass('active');
                    targetList.addClass('active').slideDown();
                }

                return false;
            });
            $(sponsorAccordionTitle[0]).addClass('active');
            $(sponsorAccordionList[0]).addClass('active').slideDown();

            // End - Sponsors accordion
        });
    </script>

